#!/usr/bin/gnuplot
set  title "Oferta de modelos por tienda"
set  ylabel "NÃºmero de modelos"
set  boxwidth 1 relative
set  style data histograms
set  style fill solid 1.0 border -1
set  xtic rotate by -45 scale 0 font ",10"
set  datafile separator "," 
set  terminal png truecolor font 'Verdana,10' size 1800,1400
set  output 'stats_models.png'
plot './stats_models.txt' using 2:xticlabels(1) notitle
